html(ng-app='app')
    head
        title Middlewares:: Session
        include ./headers/head
        script(type='text/javascript', src='/static/js/middleware-elastic.js')

    body(ng-controller='ctrl')
        include ./headers/nav
        .container
            table.table
                thead
                    tr
                        th key
                        th count
                tbody
                    tr(ng-repeat='item in list')
                        td {{item.key}}
                        td {{item.doc_count}}
